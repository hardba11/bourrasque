<?xml version="1.0" encoding="utf-8"?>
<!--
################################################################################
#
#      DEFINITION OF ALIASES OR INTERNAL PROPERTIES UPDATED BY INSTRUMENT
#
################################################################################

PROPERTY-PATH : /instrumentation/my_aircraft/tablet/
-->
<PropertyList>

  <!-- data used to control instrument -->
  <controls>
    <serviceable type="bool">1</serviceable>
    <brightness type="float">0.4</brightness>
    <page>menu</page>
    <!--
    - menu
    - map
    - checklist
    - vor
    - infos
    -->
    <map>
      <zoom type="int">6</zoom> <!-- 5 - 13 -->
    </map>
    <checklist>
      <page type="int">0</page> <!-- adjusted on tablet-model.xml, used in tablet_canvas.nas-->
    </checklist>
    <vor>
      <page type="int">0</page> <!-- adjusted on tablet-model.xml, used in tablet_canvas.nas-->
    </vor>
    <infos>
      <page type="int">0</page> <!-- adjusted on tablet-model.xml, used in tablet_canvas.nas-->
    </infos>
  </controls>

</PropertyList>

