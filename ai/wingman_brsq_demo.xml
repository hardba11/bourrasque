<?xml version="1.0" encoding="utf-8"?>

<PropertyList>
  <scenario>
    <name>Bourrasque's Wingmen</name>
    <description>
      3 wingmen - non-tactical
      This scenario formates 3 Bourrasque to the parent .ac at the position
      designated by the x,y,z offsets.
    </description>

    <entry>
      <name>Bravo Sierra</name>
      <type>wingman</type>
      <model>Aircraft/bourrasque/models/brsq.xml</model>
      <x-offset>30</x-offset>
      <y-offset>30</y-offset>
      <z-offset>-10</z-offset>
      <formate>true</formate>
      <max-speed-kts>650</max-speed-kts>
      <coefficients>
        <heading>0.0</heading>
        <pitch>0.6</pitch>
        <bank>0.7</bank>
        <speed>1.4</speed>
      </coefficients>
    </entry>

    <entry>
      <name>Bravo Lima</name>
      <type>wingman</type>
      <model>Aircraft/bourrasque/models/brsq.xml</model>
      <x-offset>-30</x-offset>
      <y-offset>-30</y-offset>
      <z-offset>10</z-offset>
      <formate>true</formate>
      <max-speed-kts>650</max-speed-kts>
      <coefficients>
        <heading>0.0</heading>
        <pitch>0.5</pitch>
        <bank>0.6</bank>
        <speed>1.2</speed>
      </coefficients>
    </entry>

