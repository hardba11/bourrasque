  <!--
================================================================================
                                                                       ANIMATION
================================================================================

________________________________________________________________________________
                                                         NOZZLE LOW RED EMISSION
  -->

  <animation>
    <type>material</type>
    <object-name>tuyere.reheat</object-name>
    <condition>
      <equals>
        <property>/engines/engine[1]/stopped</property>
        <value>0</value>
      </equals>
      <not><property>/sim/crashed</property></not>
    </condition>
    <emission>
      <factor-prop>/controls/engines/engine[1]/throttle</factor-prop>
      <red>1</red>
      <green>0.2</green>
      <blue>0.2</blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>tuyere.paroie</object-name>
    <condition>
      <equals>
        <property>/engines/engine[1]/stopped</property>
        <value>0</value>
      </equals>
      <not><property>/sim/crashed</property></not>
    </condition>
    <emission>
      <factor-prop>/controls/engines/engine[1]/throttle</factor-prop>
      <red>0.6</red>
      <green>0</green>
      <blue>0</blue>
    </emission>
  </animation>

  <!--
________________________________________________________________________________
                                                                  REHEAT EFFECTS
  -->

  <animation>
    <type>select</type>
    <object-name>reheat_inner_tuyere</object-name>
    <object-name>reheat_inner_bruleur</object-name>
    <condition>
      <and>
        <greater-than>
          <property>sim/multiplay/generic/float[9]</property>
          <value>0.1</value>
        </greater-than>
        <equals>
          <property>engines/engine[1]/stopped</property>
          <value>0</value>
        </equals>
        <not><property>/sim/crashed</property></not>
      </and>
    </condition>
  </animation>

  <animation>
    <type>material</type>
    <object-name>reheat_inner_bruleur</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>1</blue>
    </emission>
    <transparency>
      <alpha-prop>/controls/engines/engine[1]/throttle</alpha-prop>
      <factor>10</factor>
      <offset>-9</offset>
    </transparency>
  </animation>

  <animation>
    <type>material</type>
    <object-name>reheat_inner_tuyere</object-name>
    <ambient>
      <red>.6</red>
      <green>.5</green>
      <blue>1</blue>
    </ambient>
    <diffuse>
      <red>.6</red>
      <green>.5</green>
      <blue>1</blue>
    </diffuse>
    <emission>
      <red>.6</red>
      <green>.5</green>
      <blue>1</blue>
    </emission>
    <transparency>
      <alpha-prop>/controls/engines/engine[1]/throttle</alpha-prop>
      <factor>10</factor>
      <offset>-9</offset>
    </transparency>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.7</value>
      </greater-than>
      <property>/sim/rendering/shaders/skydome</property>
    </condition>
  </animation>
  <animation>
    <type>material</type>
    <object-name>reheat_inner_tuyere</object-name>
    <ambient>
      <red>1</red>
      <green>.4</green>
      <blue>0</blue>
    </ambient>
    <diffuse>
      <red>1</red>
      <green>.4</green>
      <blue>.7</blue>
    </diffuse>
    <emission>
      <red>1</red>
      <green>.4</green>
      <blue>.7</blue>
    </emission>
    <transparency>
      <alpha-prop>/controls/engines/engine[1]/throttle</alpha-prop>
      <factor>10</factor>
      <offset>-9</offset>
    </transparency>
    <condition>
      <less-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.45</value>
      </less-than>
      <property>/sim/rendering/shaders/skydome</property>
    </condition>
  </animation>
  <animation>
    <type>material</type>
    <object-name>reheat_inner_tuyere</object-name>
    <ambient>
      <red>1</red>
      <green>.4</green>
      <blue>.7</blue>
    </ambient>
    <diffuse>
      <red>1</red>
      <green>.4</green>
      <blue>.7</blue>
    </diffuse>
    <emission>
      <red>1</red>
      <green>.4</green>
      <blue>.7</blue>
    </emission>
    <transparency>
      <alpha-prop>/controls/engines/engine[1]/throttle</alpha-prop>
      <factor>10</factor>
      <offset>-9</offset>
    </transparency>
    <condition>
      <less-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.7</value>
      </less-than>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.45</value>
      </greater-than>
      <property>/sim/rendering/shaders/skydome</property>
    </condition>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>reheat_inner_tuyere</object-name>
    <property>engines/engine[1]/prop-thrust</property>
    <bias>1</bias>
    <factor>100</factor>
    <step>10</step>
    <axis>
      <x>0</x>
      <y>1</y>
    </axis>
  </animation>

