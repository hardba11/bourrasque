================================================================================
                                                                       ANIMATION
================================================================================

________________________________________________________________________________
                                                                       SHOW/HIDE
  -->

  <!-- ~~~~~~~~~~~~~~~~~~ hide head if pilot view -->
  <animation>
    <object-name>bloc_tete</object-name>
    <type>select</type>
    <condition>
      <or>
        <not-equals>
          <property>/sim/current-view/view-number</property>
          <value>0</value>
        </not-equals>
        <not-equals>
          <property>sim/model/name</property>
          <value>brsq</value>
        </not-equals>
      </or>
    </condition>
  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ hide body and arms if view side panels -->
  <animation>
    <object-name>bloc_pilote</object-name>
    <type>select</type>
    <condition>
      <or>
        <not-equals>
          <property>/sim/current-view/view-number</property>
          <value>0</value>
        </not-equals>
        <and>
          <equals>
            <property>/sim/current-view/view-number</property>
            <value>0</value>
          </equals>
          <or>
            <greater-than>
              <property>/sim/current-view/pitch-offset-deg</property>
              <value>-40</value>
            </greater-than>
            <less-than>
              <property>/sim/current-view/heading-offset-deg</property>
              <value>60</value>
            </less-than>
            <greater-than>
              <property>/sim/current-view/heading-offset-deg</property>
              <value>300</value>
            </greater-than>
          </or>
        </and>
      </or>
    </condition>
  </animation>

