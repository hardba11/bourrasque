  <!--
================================================================================
                                                             OBJECTS DECLARATION
================================================================================
  -->

  <!-- ~~~~~~~~~~~~~~~~~~ opaque + transparent -->
  <animation>
    <type>material</type>
    <!-- fixed parts -->
    <object-name>cockpit</object-name>
    <object-name>cockpit.accessoires</object-name>
    <object-name>cockpit.joint</object-name>
    <object-name>cockpit.vis_bordure</object-name>
    <object-name>cockpit.vis_panneau_central_ar</object-name>
    <object-name>cockpit.vis_panneau_central_av</object-name>
    <object-name>cockpit.vis_panneaux_lateraux</object-name>
    <object-name>support_hotas_L</object-name>
    <object-name>support_hotas_R</object-name>
    <!-- mobiles parts -->
    <object-name>hotas_L</object-name>
    <object-name>hotas_R</object-name>
    <object-name>palonnier_L</object-name>
    <object-name>palonnier_R</object-name>
    <object-name>verrin_canopy</object-name>
    <object-name>piston_canopy</object-name>
  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ transparent -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>hud_int</object-name>
  </effect>
  <effect>
    <inherits-from>Aircraft/bourrasque/models/effects/glass/glass</inherits-from>
    <object-name>hud_ext</object-name>
  </effect>
  <animation>
    <type>select</type>
    <object-name>hud_ext</object-name>
    <condition>
      <not><property>/sim/current-view/internal</property></not>
    </condition>
  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ effects -->
  <effect>
    <inherits-from>Aircraft/bourrasque/models/effects/interior/interior-shadow</inherits-from>
    <object-name>cockpit</object-name>
    <object-name>cockpit.accessoires</object-name>
    <object-name>cockpit.joint</object-name>
    <object-name>cockpit.vis_bordure</object-name>
    <object-name>cockpit.vis_panneau_central_ar</object-name>
    <object-name>cockpit.vis_panneau_central_av</object-name>
    <object-name>cockpit.vis_panneaux_lateraux</object-name>
    <object-name>support_hotas_L</object-name>
    <object-name>support_hotas_R</object-name>
    <object-name>hotas_L</object-name>
    <object-name>hotas_R</object-name>
    <object-name>palonnier_L</object-name>
    <object-name>palonnier_R</object-name>
    <object-name>verrin_canopy</object-name>
  </effect>
  <effect>
    <inherits-from>Aircraft/bourrasque/models/effects/interior/interior-flashlight</inherits-from>
    <object-name>cockpit</object-name>
    <object-name>cockpit.accessoires</object-name>
    <object-name>cockpit.joint</object-name>
    <object-name>cockpit.vis_bordure</object-name>
    <object-name>cockpit.vis_panneau_central_ar</object-name>
    <object-name>cockpit.vis_panneau_central_av</object-name>
    <object-name>cockpit.vis_panneaux_lateraux</object-name>
    <object-name>support_hotas_L</object-name>
    <object-name>support_hotas_R</object-name>
    <object-name>hotas_L</object-name>
    <object-name>hotas_R</object-name>
    <object-name>palonnier_L</object-name>
    <object-name>palonnier_R</object-name>
    <object-name>verrin_canopy</object-name>
  </effect>
  <effect>
    <inherits-from>Aircraft/bourrasque/models/effects/aircraft/piston</inherits-from>
    <object-name>piston_canopy</object-name>
  </effect>

  <!-- ~~~~~~~~~~~~~~~~~~ emitting (flood) -->
  <animation>
    <type>material</type>
    <object-name>cockpit</object-name>
    <object-name>cockpit.accessoires</object-name>
    <object-name>cockpit.joint</object-name>
    <object-name>cockpit.vis_bordure</object-name>
    <object-name>cockpit.vis_panneau_central_ar</object-name>
    <object-name>cockpit.vis_panneau_central_av</object-name>
    <object-name>cockpit.vis_panneaux_lateraux</object-name>
    <object-name>support_hotas_L</object-name>
    <object-name>support_hotas_R</object-name>
    <object-name>hotas_L</object-name>
    <object-name>hotas_R</object-name>
    <emission>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
      <red>.5</red>
      <green>0</green>
      <blue>0</blue>
    </emission>
    <condition>
      <property>systems/electrical/bus/avionics</property>
    </condition>
  </animation>
  <animation>
    <type>material</type>
    <texture>texture_cockpit_flood.png</texture>
    <object-name>cockpit</object-name>
    <object-name>cockpit.accessoires</object-name>
    <object-name>cockpit.joint</object-name>
    <object-name>cockpit.vis_bordure</object-name>
    <object-name>cockpit.vis_panneau_central_ar</object-name>
    <object-name>cockpit.vis_panneau_central_av</object-name>
    <object-name>cockpit.vis_panneaux_lateraux</object-name>
    <object-name>support_hotas_L</object-name>
    <object-name>support_hotas_R</object-name>
    <condition>
      <less-than>
        <property>/rendering/scene/diffuse/green</property>
        <value>.12</value>
      </less-than>
      <greater-than>
        <property>/controls/lighting/panel-norm</property>
        <value>.39</value>
      </greater-than>
      <property>systems/electrical/bus/avionics</property>
    </condition>
  </animation>

  <!-- ~~~~~~~~~~~~~~~~~~ emitting (color) -->
  <animation>
    <type>material</type>
    <object-name>hud_ext</object-name>
    <emission>
      <red>0</red>
      <green>.8</green>
      <blue>.1</blue>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>hud_int</object-name>
    <emission>
      <red>.45</red>
      <green>.05</green>
      <blue>.35</blue>
    </emission>
  </animation>

