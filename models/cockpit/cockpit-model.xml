  <!--
________________________________________________________________________________
                                                                     RIGHT HOTAS
  -->
  <animation>
    <type>rotate</type>
    <object-name>hotas_R</object-name>
    <property>/controls/flight/elevator</property>
    <factor>8</factor>
    <center>
      <x-m>-3.5072</x-m>
      <y-m>0</y-m>
      <z-m>.4044</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>hotas_R</object-name>
    <property>/controls/flight/aileron</property>
    <factor>8</factor>
    <center>
      <x-m>-3.5072</x-m>
      <y-m>.3997</y-m>
      <z-m>.4044</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <!--
________________________________________________________________________________
                                                                      LEFT HOTAS
  -->
  <animation>
    <type>translate</type>
    <object-name>hotas_L</object-name>
    <property>/controls/engines/engine[0]/throttle</property>
    <factor>.06</factor>
    <axis>
      <x>-.118</x>
      <y>.011</y>
      <z>.009</z>
    </axis>
  </animation>

  <!--
________________________________________________________________________________
                                                                  LEFT PALONNIER
  -->
  <!-- ~~~~~~~~~~~~~~~~~~ rudder -->
  <animation>
    <type>translate</type>
    <object-name>palonnier_L</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>.03</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <!-- ~~~~~~~~~~~~~~~~~~ brakes -->
  <animation>
    <type>rotate</type>
    <object-name>palonnier_L</object-name>
    <property>/gear/gear[1]/brake-position-norm</property>
    <factor>10</factor>
    <center>
      <x-m>-4.186</x-m>
      <y-m>0</y-m>
      <z-m>.295</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <!--
________________________________________________________________________________
                                                                 RIGHT PALONNIER
  -->
  <!-- ~~~~~~~~~~~~~~~~~~ rudder -->
  <animation>
    <type>translate</type>
    <object-name>palonnier_R</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>.03</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <!-- ~~~~~~~~~~~~~~~~~~ brakes -->
  <animation>
    <type>rotate</type>
    <object-name>palonnier_R</object-name>
    <property>/gear/gear[2]/brake-position-norm</property>
    <factor>10</factor>
    <center>
      <x-m>-4.186</x-m>
      <y-m>0</y-m>
      <z-m>.295</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <!--
________________________________________________________________________________
                                                                          CANOPY
  -->

  <!-- ~~~~~~~~~~~~~~~~~~ verrin canopy -->
  <animation>
    <type>rotate</type>
    <object-name>verrin_canopy</object-name>
    <object-name>piston_canopy</object-name>
    <property>sim/multiplay/generic/float[0]</property> <!-- for MP sim/model/canopy-pos-norm -->
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>.1</ind><dep>0</dep></entry>
      <entry><ind>.2</ind><dep>2.5</dep></entry>
      <entry><ind>.6</ind><dep>2.5</dep></entry>
      <entry><ind>.7</ind><dep>2.2</dep></entry>
      <entry><ind>.8</ind><dep>2.3</dep></entry>
      <entry><ind>.9</ind><dep>2.5</dep></entry>
      <entry><ind>1</ind><dep>3.2</dep></entry>
    </interpolation>
    <center>
      <x-m>-1.431</x-m>
      <y-m>0</y-m>
      <z-m>.928</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <!-- ~~~~~~~~~~~~~~~~~~ translating piston -->
  <animation>
    <type>translate</type>
    <object-name>piston_canopy</object-name>
    <property>sim/multiplay/generic/float[0]</property> <!-- for MP sim/model/canopy-pos-norm -->
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>.1</ind><dep>0</dep></entry>
      <entry><ind>.2</ind><dep>.015</dep></entry>
      <entry><ind>.6</ind><dep>.02</dep></entry>
      <entry><ind>.7</ind><dep>.038</dep></entry>
      <entry><ind>.8</ind><dep>.055</dep></entry>
      <entry><ind>.9</ind><dep>.072</dep></entry>
      <entry><ind>1</ind><dep>.09</dep></entry>
    </interpolation>
    <axis>
      <x>.6</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <!--
================================================================================
                                                                 PLACEMENT PILOT
================================================================================

                                                          TOP
                                                           Z
                                                           ^
                                                           .
                                                           .
                         + nd (x=-3.83,z=.835)             .
                                                           .
                     +  ai (x=-3.92,z=.54)                 .
                            +  efis (x=-3.79,z=.348)       .
                                                           .
  FRONT ...................................................0........> X BACK
                                                           .

________________________________________________________________________________
                                                                            SEAT
  -->
  <!-- ~~~~~~~~~~~~~~~~~~ seat -->
  <model>
    <name>siege-pilot</name>
    <path>Aircraft/bourrasque/models/siege/siege-model.xml</path>
  </model>

  <!-- ~~~~~~~~~~~~~~~~~~ pilot -->
  <model>
    <name>pilot</name>
    <path>Aircraft/bourrasque/models/pilot/pilot.xml</path>
    <condition>
      <or>
        <property>controls/pax/pilot</property>
        <greater-than>
          <property>sim/multiplay/generic/float[4]</property>
          <value>1</value>
        </greater-than>
      </or>
    </condition>
  </model>

<!--
________________________________________________________________________________
                                                            HUD TARGET POSITIONS

@see include/sim-view0-properties.xml
@see instruments/hud_target_positions/
diameter = 2m
offsets and center : has to be the coords for view n=0
* x-m = z-offset-m
* z-m = y-offset-m
default-field-of-view-deg has to be set on 70.0
  -->
  <model>
    <name>hud_target_positions</name>
    <path>Aircraft/bourrasque/instruments/hud_target_positions/hud_target_positions-model.xml</path>
    <offsets>
      <x-m>-3.32</x-m>
      <y-m>0</y-m>
      <z-m>1.15</z-m>
    </offsets>
  </model>
  <animation>
    <type>rotate</type> <!-- left/right -->
    <object-name>hud_target_positions</object-name>
    <property>/instrumentation/my_aircraft/hud_target_positions/view/heading-offset-deg</property>
    <center>
      <x-m>-3.32</x-m>
      <y-m>0</y-m>
      <z-m>1.15</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type> <!-- up/down -->
    <object-name>hud_target_positions</object-name>
    <property>/instrumentation/my_aircraft/hud_target_positions/view/pitch-offset-deg</property>
    <center>
      <x-m>-3.32</x-m>
      <y-m>0</y-m>
      <z-m>1.15</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

