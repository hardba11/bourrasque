    <center>
      <x-m>-2.677</x-m>
      <y-m>0</y-m>
      <z-m>-0.941</z-m>
    </center>
    <axis>
      <x>0.023</x>
      <y>0</y>
      <z>0.117</z>
    </axis>
  </animation>

