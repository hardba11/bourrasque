      <entry><ind>0.19</ind><dep>0</dep></entry>
      <entry><ind>0.4</ind><dep>20</dep></entry>
    </interpolation>
    <center>
      <x-m>-2.6749</x-m>
      <y-m>0</y-m>
      <z-m>-1.3096</z-m>
