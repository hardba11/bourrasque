  <animation>
    <type>rotate</type>
    <object-name>compas_jambe_train</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind>0</ind><dep>25</dep></entry>
      <entry><ind>0.19</ind><dep>0</dep></entry>
      <entry><ind>0.4</ind><dep>-20</dep></entry>
    </interpolation>
    <center>
      <x-m>-2.6402</x-m>
      <y-m>0</y-m>
      <z-m>-1.082</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

